<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ourcw">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OurCw">
        <activity
            android:name=".TeacherClassAssignmentPage"
            android:exported="false" />
        <activity
            android:name=".TeacherPanelPageActivity"
            android:exported="false" />
        <activity
            android:name=".StudentPanelPageActivity"
            android:exported="false" />
        <activity
            android:name=".TeacherAssignmentPageActivity"
            android:exported="false" />
        <activity
            android:name=".StudentAssignmentPageActivity"
            android:exported="false"
            android:parentActivityName=".ClassroomActivity" />
        <activity
            android:name=".ClassroomActivity"
            android:exported="false" />
        <activity
            android:name=".TeacherRegistrationActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".StudentRegistrationActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>